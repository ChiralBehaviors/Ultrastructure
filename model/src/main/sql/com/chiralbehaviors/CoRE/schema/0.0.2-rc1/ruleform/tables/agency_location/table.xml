    <changeSet author="hhildebrand" id="1390879803225-36">
        <createTable schemaName="ruleform" tableName="agency_location">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="agency_location_pkey" />
            </column>
            <column name="authority" type="CHAR(22)" defaultValue="AAAAAAAAAAAAAAAAAAAAAQ">
