    <changeSet author="hhildebrand" id="1390879803225-47">
        <createTable remarks="Defines rules stating what attributes (and values thereof) are authorized for particular Protocols"
            tableName="ruleform.protocol_attribute_authorization"
        >
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="protocol_attribute_authorization_pkey" />
            </column>
            <column name="requester" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="requester_classification" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="deliver_from" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="deliver_from_classification" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="deliver_to" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="deliver_to_classification" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product_classification" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="service" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="service_classification" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="authorized_attribute" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="1" name="sequence_number" type="ruleform.sequence_number">
                <constraints nullable="false" />
            </column>
            <column name="grouping_agency" type="INT8" />
            <column name="text_value" type="VARCHAR(10485760)" />
            <column name="integer_value" type="INT4" />
            <column name="numeric_value" type="numeric" />
            <column name="timestamp_value" type="TIMESTAMPTZ" />
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.protocol_attribute_authorization" />
        </rollback>
    </changeSet>
