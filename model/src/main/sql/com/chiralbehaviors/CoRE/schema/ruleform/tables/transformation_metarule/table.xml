<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-59">
        <createTable tableName="ruleform.transformation_metarule">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="transformation_metarule_pkey" />
            </column>
            <column name="service" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="sequence_number" type="ruleform.sequence_number">
                <constraints nullable="false" />
            </column>
            <column name="agency_map" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product_map" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product_network_agency" type="INT8" />
            <column defaultValueNumeric="0" name="stop_on_match" type="SMALLINT" />
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.transformation_metarule" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-343">
        <addForeignKeyConstraint baseColumnNames="agency_map" baseTableName="ruleform.transformation_metarule"
            constraintName="transformation_metarule_agency_map_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-344">
        <addForeignKeyConstraint baseColumnNames="product_map" baseTableName="ruleform.transformation_metarule"
            constraintName="transformation_metarule_product_map_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-345">
        <addForeignKeyConstraint baseColumnNames="product_network_agency" baseTableName="ruleform.transformation_metarule"
            constraintName="transformation_metarule_product_network_agency_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-346">
        <addForeignKeyConstraint baseColumnNames="service" baseTableName="ruleform.transformation_metarule"
            constraintName="transformation_metarule_service_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-416">
        <addUniqueConstraint columnNames="service, sequence_number" constraintName="transformation_metarule_service_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="ruleform.transformation_metarule" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-522">
        <createIndex indexName="transformation_metarule_updated_by_fkey_idx" tableName="ruleform.transformation_metarule"
            unique="false"
        >
            <column name="updated_by" />
        </createIndex>
    </changeSet>
</databaseChangeLog>
