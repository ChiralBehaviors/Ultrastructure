<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet
		id="1"
		author="hhildebrand">
		<sql><![CDATA[
			CREATE INDEX original_entity_network_idx
			ON ruleform.entity_network
			USING btree
			(id )
			WHERE distance = 1 AND updated_by <> 31; 
			]]>
		</sql>
		<rollback>
			<dropIndex tableName="ruleform.entity_network" indexName="original_entity_network_idx" />
		</rollback>
	</changeSet>
</databaseChangeLog>
