    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-376">
        <addUniqueConstraint columnNames="parent, relationship, child" constraintName="attribute_network_parent_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="ruleform.attribute_network" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-442">
        <createIndex indexName="attribute_network_child_fkey_idx" tableName="ruleform.attribute_network" unique="false">
            <column name="child" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-443">
        <createIndex indexName="attribute_network_parent_fkey_idx" tableName="ruleform.attribute_network" unique="false">
            <column name="parent" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-444">
        <createIndex indexName="attribute_network_relationship_fkey_idx" tableName="ruleform.attribute_network" unique="false">
            <column name="relationship" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-445">
        <createIndex indexName="attribute_network_updated_by_fkey_idx" tableName="ruleform.attribute_network" unique="false">
            <column name="updated_by" />
        </createIndex>
    </changeSet>
