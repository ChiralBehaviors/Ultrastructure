<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="1"
        author="hhildebrand">
        <sql> CREATE TRIGGER meta_protocol_update_date BEFORE INSERT OR UPDATE ON
            ruleform.meta_protocol FOR EACH ROW EXECUTE PROCEDURE ruleform.set_update_date(); </sql>
        <rollback> DROP TRIGGER meta_protocol_update_date ON ruleform.meta_protocol; </rollback>
    </changeSet>
</databaseChangeLog>
