<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="1"
        author="hhildebrand">
        <sql> CREATE TRIGGER location_relationship_update_date BEFORE INSERT OR UPDATE ON
            ruleform.location_relationship FOR EACH ROW EXECUTE PROCEDURE ruleform.set_update_date(); </sql>
        <rollback> DROP TRIGGER location_relationship_update_date ON ruleform.location_relationship; </rollback>
    </changeSet>
</databaseChangeLog>
