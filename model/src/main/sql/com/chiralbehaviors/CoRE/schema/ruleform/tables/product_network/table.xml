            referencedColumnNames="id" referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-402">
        <addUniqueConstraint columnNames="parent, relationship, child" constraintName="product_network_key" deferrable="false"
            disabled="false" initiallyDeferred="false" tableName="ruleform.product_network" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-503">
        <createIndex indexName="product_network_child_fkey_idx" tableName="ruleform.product_network" unique="false">
            <column name="child" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-504">
        <createIndex indexName="product_network_parent_fkey_idx" tableName="ruleform.product_network" unique="false">
            <column name="parent" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-505">
        <createIndex indexName="product_network_relationship_fkey_idx" tableName="ruleform.product_network" unique="false">
