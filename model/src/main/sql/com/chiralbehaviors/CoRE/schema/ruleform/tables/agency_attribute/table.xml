<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-3">
        <createTable tableName="ruleform.agency_attribute">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="agency_attribute_pkey" />
            </column>
            <column name="agency" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="attribute" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="numeric_value" type="numeric" />
            <column name="integer_value" type="INT4" />
            <column name="boolean_value" type="SMALLINT" />
            <column name="text_value" type="VARCHAR(10485760)" />
            <column name="timestamp_value" type="TIMESTAMP WITH TIME ZONE" />
            <column name="binary_value" type="BYTEA" />
            <column name="unit" type="INT8" />
            <column defaultValueComputed="1" name="sequence_number" type="ruleform.sequence_number">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.agency_attribute" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-87">
        <addForeignKeyConstraint baseColumnNames="agency" baseTableName="ruleform.agency_attribute" constraintName="agency_attribute_agency_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-88">
        <addForeignKeyConstraint baseColumnNames="attribute" baseTableName="ruleform.agency_attribute"
            constraintName="agency_attribute_attribute_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-95">
        <addForeignKeyConstraint baseColumnNames="updated_by" baseTableName="ruleform.agency_attribute"
            constraintName="agency_attribute_updated_by_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-94">
        <addForeignKeyConstraint baseColumnNames="unit" baseTableName="ruleform.agency_attribute" constraintName="agency_attribute_unit_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.unit" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-369">
        <addUniqueConstraint columnNames="agency, attribute" constraintName="agency_attribute_agency_key" deferrable="false"
            disabled="false" initiallyDeferred="false" tableName="ruleform.agency_attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-426">
        <createIndex indexName="agency_attribute_updated_by_fkey_idx" tableName="ruleform.agency_attribute" unique="false">
            <column name="updated_by" />
        </createIndex>
    </changeSet>
</databaseChangeLog>
