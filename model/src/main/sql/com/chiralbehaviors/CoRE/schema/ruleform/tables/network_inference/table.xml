    <changeSet author="hhildebrand" id="1390879803225-31">
        <createTable schemaName="ruleform" tableName="network_inference">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="network_inference_pkey" />
            </column>
            <column name="premise1" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="premise2" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="inference" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" defaultValue="AAAAAAAAAAAAAAAAAAAAAQ" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="network_inference" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="879803225-208">
