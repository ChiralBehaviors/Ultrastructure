<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-30">
        <createTable schemaName="ruleform" remarks="Rules for generating masks for the protocol ruleform" tableName="meta_protocol">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="meta_protocol_pkey" />
            </column>
            <column name="service" remarks="The service we are creating a mask for" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="sequence_number" remarks="Ordering and uniqueness, for when a service can have multiple meta-protocol rules"
                type="sequence_number"
            >
                <constraints nullable="false" />
            </column>
            <column name="requesting_agency" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="service_type" remarks="Used to select a mapped value from a Product" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="product_ordered" remarks="Used to select a mapped value from a Product" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="deliver_to" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="deliver_from" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column defaultValueNumeric="0" name="stop_on_match" type="SMALLINT" />
            <column name="notes" remarks="Additional notes regarding usage of the attribute" type="VARCHAR(10485760)" />
            <column name="updated_by" remarks="The agency that last updated this rule" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" remarks="The date this rule was last changed"
                type="TIMESTAMPTZ"
            >
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="meta_protocol" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-201">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="deliver_from" baseTableName="meta_protocol"
            constraintName="meta_protocol_deliver_from_fkey" deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-202">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="deliver_to" baseTableName="meta_protocol" constraintName="meta_protocol_deliver_to_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-203">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="product_ordered" baseTableName="meta_protocol"
            constraintName="meta_protocol_product_ordered_fkey" deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-204">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="requesting_agency" baseTableName="meta_protocol"
            constraintName="meta_protocol_requesting_agency_fkey" deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-205">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="service" baseTableName="meta_protocol" constraintName="meta_protocol_service_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-206">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="service_type" baseTableName="meta_protocol"
            constraintName="meta_protocol_service_type_fkey" deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-207">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="updated_by" baseTableName="meta_protocol" constraintName="meta_protocol_updated_by_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-391">
        <addUniqueConstraint schemaName="ruleform" columnNames="service, sequence_number" constraintName="meta_protocol_service_sequence_number_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="meta_protocol" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-479">
        <createIndex schemaName="ruleform" indexName="meta_protocol_updated_by_fkey_idx" tableName="meta_protocol" unique="false">
            <column name="updated_by" />
        </createIndex>
    </changeSet>
</databaseChangeLog>
