<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <appender name="C" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>${testing.logging.consoleLayoutPattern}</pattern>
        </encoder>
    </appender>
    <logger name="org.jooq">
        <level value="DEBUG" />
    </logger> 
    <root>
        <level value="ERROR" />
        <appender-ref ref="C" />
    </root>
</configuration>
