<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet author="hhildebrand" id="1390879803225-7">
        <createTable tableName="ruleform.agency_product">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="agency_product_pkey"/>
            </column>
            <column name="agency" type="INT8">
                <constraints nullable="false"/>
            </column>
            <column name="relationship" type="INT8">
                <constraints nullable="false"/>
            </column>
            <column name="product" type="INT8">
                <constraints nullable="false"/>
            </column>
            <column name="notes" type="VARCHAR(10485760)"/>
            <column name="updated_by" type="INT8">
                <constraints nullable="false"/>
            </column>
            <column defaultValueComputed="('NOW'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.agency_product"/>
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-106">
        <addForeignKeyConstraint baseColumnNames="agency" baseTableName="ruleform.agency_product" constraintName="agency_product_agency_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.agency"/>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-107">
        <addForeignKeyConstraint baseColumnNames="product" baseTableName="ruleform.agency_product" constraintName="agency_product_product_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.product"/>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-108">
        <addForeignKeyConstraint baseColumnNames="relationship" baseTableName="ruleform.agency_product" constraintName="agency_product_relationship_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.relationship"/>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-109">
        <addForeignKeyConstraint baseColumnNames="updated_by" baseTableName="ruleform.agency_product" constraintName="agency_product_updated_by_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.agency"/>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-373">
        <addUniqueConstraint columnNames="agency, relationship, product" constraintName="agency_product_agency_key" deferrable="false" disabled="false" initiallyDeferred="false" tableName="ruleform.agency_product"/>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-432">
        <createIndex indexName="agency_product_agency_fkey_idx" tableName="ruleform.agency_product" unique="false">
            <column name="agency"/>
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-433">
        <createIndex indexName="agency_product_product_fkey_idx" tableName="ruleform.agency_product" unique="false">
            <column name="product"/>
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-434">
        <createIndex indexName="agency_product_relationship_fkey_idx" tableName="ruleform.agency_product" unique="false">
            <column name="relationship"/>
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-435">
        <createIndex indexName="agency_product_updated_by_fkey_idx" tableName="ruleform.agency_product" unique="false">
            <column name="updated_by"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>
