<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-36">
        <createTable tableName="ruleform.product_location">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="product_location_pkey" />
            </column>
            <column name="agency" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="relationship" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="location" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.product_location" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-226">
        <addForeignKeyConstraint baseColumnNames="agency" baseTableName="ruleform.product_location" constraintName="product_location_agency_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-237">
        <addForeignKeyConstraint baseColumnNames="location" baseTableName="ruleform.product_location"
            constraintName="product_location_location_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.location" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-243">
        <addForeignKeyConstraint baseColumnNames="product" baseTableName="ruleform.product_location" constraintName="product_location_product_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-244">
        <addForeignKeyConstraint baseColumnNames="relationship" baseTableName="ruleform.product_location"
            constraintName="product_location_relationship_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-245">
        <addForeignKeyConstraint baseColumnNames="updated_by" baseTableName="ruleform.product_location"
            constraintName="product_location_updated_by_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-397">
        <addUniqueConstraint columnNames="agency, product, relationship, location" constraintName="product_location_agency_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="ruleform.product_location" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-489">
        <createIndex indexName="product_location_location_fkey_idx" tableName="ruleform.product_location" unique="false">
            <column name="location" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-493">
        <createIndex indexName="product_location_product_fkey_idx" tableName="ruleform.product_location" unique="false">
            <column name="product" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-494">
        <createIndex indexName="product_location_relationship_fkey_idx" tableName="ruleform.product_location" unique="false">
            <column name="relationship" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-495">
        <createIndex indexName="product_location_updated_by_fkey_idx" tableName="ruleform.product_location" unique="false">
            <column name="updated_by" />
        </createIndex>
    </changeSet>
</databaseChangeLog>
