<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<changeSet author="hhildebrand" id="2.0.0">
		<createTable schemaName="ruleform"
			tableName="existential_network">
			<column name="workspace" type="uuid" />
			<column name="id" type="uuid">
				<constraints primaryKey="true"
					primaryKeyName="existential_network_pkey" nullable="false" />
			</column>
			<column name="parent" type="uuid">
				<constraints nullable="false" />
			</column>
			<column name="relationship" type="uuid">
				<constraints nullable="false" />
			</column>
			<column name="child" type="uuid">
				<constraints nullable="false" />
			</column>
			<column name="inference" type="uuid" />
			<column name="premise1" type="uuid" />
			<column name="premise2" type="uuid" />
			<column name="notes" type="TEXT" />
			<column name="updated_by"
				defaultValue="'00000000-0000-0000-0000-000000000003'" type="uuid">
				<constraints nullable="false" />
			</column>
			<column name="authority" type="uuid" />
			<column name="version" type="INT4" />
		</createTable>

		<addUniqueConstraint schemaName="ruleform"
			columnNames="parent, relationship, child"
			constraintName="existential_network_p_r_c_unique" deferrable="false"
			disabled="false" initiallyDeferred="false"
			tableName="existential_network" />

		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="child" baseTableName="existential_network"
			constraintName="existential_network_child_fkey" deferrable="true"
			initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="parent" baseTableName="existential_network"
			constraintName="existential_network_parent_fkey" deferrable="true"
			initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="relationship" baseTableName="existential_network"
			constraintName="existential_network_relationship_fkey"
			deferrable="true" initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="updated_by" baseTableName="existential_network"
			constraintName="existential_network_updated_by_fkey"
			deferrable="true" initiallyDeferred="true" onDelete="SET DEFAULT"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="authority" baseTableName="existential_network"
			constraintName="existential_network_authority_fkey" deferrable="true"
			initiallyDeferred="true" onDelete="SET DEFAULT"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="premise1" baseTableName="existential_network"
			constraintName="existential_network_premise1_fkey" deferrable="true"
			initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential_network" />
		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="premise2" baseTableName="existential_network"
			constraintName="existential_network_premise2_fkey" deferrable="true"
			initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential_network" />
		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="inference" baseTableName="existential_network"
			constraintName="existential_network_inference_fkey" deferrable="true"
			initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="network_inference" />
		<addForeignKeyConstraint
			baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
			baseColumnNames="workspace" baseTableName="existential_network"
			constraintName="existential_network_workspace_fkey" deferrable="true"
			initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential" />

		<createIndex schemaName="ruleform"
			indexName="existential_network_relationship_idx"
			tableName="existential_network" unique="false">
			<column name="relationship" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_network_child_fky_idx"
			tableName="existential_network" unique="false">
			<column name="child" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_network_premise2_fky_idx"
			tableName="existential_network" unique="false">
			<column name="premise2" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_network_inference_fky_idx"
			tableName="existential_network" unique="false">
			<column name="inference" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_network_workspace_fkey_idx"
			tableName="existential_network" unique="false">
			<column name="workspace" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_network_updated_by_fkey_idx"
			tableName="existential_network" unique="false">
			<column name="updated_by" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_network_authority_fkey_idx"
			tableName="existential_network" unique="false">
			<column name="authority" />
		</createIndex>
	</changeSet>
</databaseChangeLog>
