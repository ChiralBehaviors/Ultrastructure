<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<appender name="C" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>${testing.logging.consoleLayoutPattern}</pattern>
		</encoder>
	</appender>
	<logger name="com.chiralbehaviors.CoRE">
		<level value="TRACE" />
	</logger>
	<logger name="org.jooq">
		<level value="info" />
	</logger>
    <logger name="org.jooq.tools.LoggerListener">
        <level value="INFO" />
    </logger> 
	<root>
		<level value="INFO" />
		<appender-ref ref="C" />
	</root>
</configuration>
