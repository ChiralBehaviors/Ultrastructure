<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<changeSet author="hhildebrand" id="2.0.0">
		<createTable schemaName="ruleform" tableName="existential_attribute">
			<column name="workspace" type="uuid" />
			<column name="id" type="uuid">
				<constraints primaryKey="true" primaryKeyName="existential_attribute_pkey" />
			</column>
			<column name="existential" type="uuid">
				<constraints nullable="false" />
			</column>
			<column name="attribute" type="uuid">
				<constraints nullable="false" />
			</column>
			<column name="key" type="TEXT" />
			<column name="numeric_value" type="numeric" />
			<column name="integer_value" type="INT4" />
			<column name="boolean_value" type="boolean" />
			<column name="text_value" type="TEXT" />
			<column name="timestamp_value" type="TIMESTAMP WITH TIME ZONE" />
			<column name="binary_value" type="BYTEA" />
			<column name="unit" type="uuid" />
			<column name="sequence_number" type="INT4" />
			<column name="notes" type="TEXT" />
			<column name="updated_by" defaultValue="'00000000-0000-0000-0000-000000000003'"
				type="uuid">
				<constraints nullable="false" />
			</column>
			<column name="version" type="INT4" />
			<column name="updated" type="TIMESTAMP WITH TIME ZONE"
				defaultValueComputed="current_timestamp">
				<constraints nullable="false" />
			</column>
			<column name="json_value" type="jsonb" />
			<column name="authority" type="uuid" />
		</createTable>
		<addForeignKeyConstraint baseTableSchemaName="ruleform "
			referencedTableSchemaName="ruleform" baseColumnNames="existential"
			baseTableName="existential_attribute" constraintName="existential_attribute_existential_fkey"
			deferrable="true" initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint baseTableSchemaName="ruleform "
			referencedTableSchemaName="ruleform" baseColumnNames="attribute"
			baseTableName="existential_attribute" constraintName="existential_attribute_attribute_fkey"
			deferrable="true" initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint baseTableSchemaName="ruleform "
			referencedTableSchemaName="ruleform" baseColumnNames="updated_by"
			baseTableName="existential_attribute" constraintName="existential_attribute_updated_by_fkey"
			deferrable="true" initiallyDeferred="true" onDelete="SET DEFAULT"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint baseTableSchemaName="ruleform "
			referencedTableSchemaName="ruleform" baseColumnNames="unit"
			baseTableName="existential_attribute" constraintName="existential_attribute_unit_fkey"
			deferrable="true" initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential" />
		<addForeignKeyConstraint baseTableSchemaName="ruleform "
			referencedTableSchemaName="ruleform" baseColumnNames="authority"
			baseTableName="existential_attribute"
			constraintName="existential_attribute_authorization_grouping_agency_fkey"
			deferrable="false" initiallyDeferred="true" onDelete="CASCADE"
			referencedColumnNames="id" referencedTableName="existential" />
        <addForeignKeyConstraint baseTableSchemaName="ruleform "
            referencedTableSchemaName="ruleform" baseColumnNames="workspace"
            baseTableName="existential_attribute" constraintName="existential_attribute_workspace_fkey"
            deferrable="true" initiallyDeferred="true" onDelete="CASCADE"
            referencedColumnNames="id" referencedTableName="existential" />
        <addUniqueConstraint schemaName="ruleform"
            columnNames="existential, attribute, sequence_number" constraintName="existential_attribute_agency_key"
            deferrable="false" disabled="false" initiallyDeferred="false"
            tableName="existential_attribute" />
		<createIndex schemaName="ruleform"
			indexName="existential_attribute_updated_by_fkey_idx" tableName="existential_attribute"
			unique="false">
			<column name="updated_by" />
		</createIndex>
		<createIndex schemaName="ruleform" indexName="existential_attribute_key_idx"
			tableName="existential_attribute" unique="false">
			<column name="key" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_attribute_numeric_idx" tableName="existential_attribute"
			unique="false">
			<column name="numeric_value" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_attribute_timestamp_idx" tableName="existential_attribute"
			unique="false">
			<column name="timestamp_value" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_attribute_existential_idx" tableName="existential_attribute"
			unique="false">
			<column name="existential" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_attribute_attribute_idx" tableName="existential_attribute"
			unique="false">
			<column name="attribute" />
		</createIndex>
		<createIndex schemaName="ruleform" indexName="existential_attribute_unit_idx"
			tableName="existential_attribute" unique="false">
			<column name="unit" />
		</createIndex>
		<createIndex schemaName="ruleform"
			indexName="existential_attribute_authority_idx" tableName="existential_attribute"
			unique="false">
			<column name="authority" />
		</createIndex>
	</changeSet>
</databaseChangeLog>
