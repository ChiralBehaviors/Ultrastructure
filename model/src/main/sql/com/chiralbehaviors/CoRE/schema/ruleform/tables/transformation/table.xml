<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-58">
        <createTable tableName="ruleform.transformation">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="transformation_pkey" />
            </column>
            <column name="service" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="agency" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="sequence_number" type="ruleform.sequence_number">
                <constraints nullable="false" />
            </column>
            <column name="assign_to" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="agency_key" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product_key" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="relationship_key" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="attribute" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product_attribute_agency" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.transformation" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-339">
        <addForeignKeyConstraint baseColumnNames="agency" baseTableName="ruleform.transformation" constraintName="transformation_agency_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-340">
        <addForeignKeyConstraint baseColumnNames="agency_key" baseTableName="ruleform.transformation"
            constraintName="transformation_agency_key_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-341">
        <addForeignKeyConstraint baseColumnNames="assign_to" baseTableName="ruleform.transformation" constraintName="transformation_assign_to_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-342">
        <addForeignKeyConstraint baseColumnNames="attribute" baseTableName="ruleform.transformation" constraintName="transformation_attribute_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-347">
        <addForeignKeyConstraint baseColumnNames="product_attribute_agency" baseTableName="ruleform.transformation"
            constraintName="transformation_product_attribute_agency_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-348">
        <addForeignKeyConstraint baseColumnNames="product" baseTableName="ruleform.transformation" constraintName="transformation_product_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-349">
        <addForeignKeyConstraint baseColumnNames="product_key" baseTableName="ruleform.transformation"
            constraintName="transformation_product_key_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-350">
        <addForeignKeyConstraint baseColumnNames="relationship_key" baseTableName="ruleform.transformation"
            constraintName="transformation_relationship_key_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-351">
        <addForeignKeyConstraint baseColumnNames="service" baseTableName="ruleform.transformation" constraintName="transformation_service_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-417">
        <addUniqueConstraint columnNames="service, agency, product, sequence_number" constraintName="transformation_service_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="ruleform.transformation" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-523">
        <createIndex indexName="transformation_updated_by_fkey_idx" tableName="ruleform.transformation" unique="false">
            <column name="updated_by" />
        </createIndex>
    </changeSet>
</databaseChangeLog>
