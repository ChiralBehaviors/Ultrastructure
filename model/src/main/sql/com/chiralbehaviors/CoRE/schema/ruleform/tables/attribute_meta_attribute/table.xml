<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-10">
        <createTable schemaName="ruleform" tableName="attribute_meta_attribute">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="attribute_attribute_pkey" />
            </column>
            <column name="attribute" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="meta_attribute" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="numeric_value" type="numeric" />
            <column name="boolean_value" type="SMALLINT" />
            <column name="text_value" type="VARCHAR(10485760)" />
            <column name="binary_value" type="BYTEA" />
            <column name="attribute_value" type="CHAR(22)" />
            <column name="integer_value" type="INT4" />
            <column name="timestamp_value" type="TIMESTAMP WITH TIME ZONE" />
            <column name="unit_value" type="CHAR(22)" />
            <column name="unit" type="CHAR(22)" />
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="1" name="sequence_number" type="sequence_number">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="attribute_meta_attribute_kind" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-116">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="meta_attribute" baseTableName="attribute_meta_attribute"
            constraintName="attribute_attribute_attribute_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-117">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="attribute_value" baseTableName="attribute_meta_attribute"
            constraintName="attribute_attribute_attribute_value_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-118">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="attribute" baseTableName="attribute_meta_attribute"
            constraintName="attribute_attribute_base_attribute_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-119">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="unit" baseTableName="attribute_meta_attribute"
            constraintName="attribute_attribute_unit_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="unit" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-120">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="unit_value" baseTableName="attribute_meta_attribute"
            constraintName="attribute_attribute_unit_value_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="unit" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-121">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"  baseColumnNames="updated_by" baseTableName="attribute_meta_attribute"
            constraintName="attribute_attribute_updated_by_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-375">
        <addUniqueConstraint schemaName="ruleform" columnNames="attribute, meta_attribute, sequence_number" constraintName="attribute_attribute_base_attribute_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="attribute_meta_attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-438">
        <createIndex schemaName="ruleform" indexName="attribute_attribute_attribute_fkey_idx" tableName="attribute_meta_attribute" unique="false">
            <column name="meta_attribute" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-439">
        <createIndex schemaName="ruleform" indexName="attribute_attribute_attribute_value_fkey_idx" tableName="attribute_meta_attribute"
            unique="false"
        >
            <column name="attribute_value" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-440">
