>

    <changeSet author="hhildebrand" id="1390879803225-40">
        <createTable tableName="ruleform.product_network">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="product_network_pkey" />
            </column>
            <column name="parent" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="relationship" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="child" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueNumeric="0" name="inferred" type="SMALLINT">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
            <column name="ordering" type="ruleform.ordinal_number" />
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.product_network" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-258">
        <addForeignKeyConstraint baseColumnNames="child" baseTableName="ruleform.product_network" constraintName="product_network_child_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-259">
        <addForeignKeyConstraint baseColumnNames="parent" baseTableName="ruleform.product_network" constraintName="product_network_parent_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-260">
        <addForeignKeyConstraint baseColumnNames="relationship" baseTableName="ruleform.product_network"
            constraintName="product_network_relationship_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-261">
        <addForeignKeyConstraint baseColumnNames="updated_by" baseTableName="ruleform.product_network"
