<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-49">
        <createTable tableName="ruleform.relationship_attribute">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="relationship_attribute_pkey" />
            </column>
            <column name="relationship" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="attribute" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="numeric_value" type="numeric" />
            <column name="integer_value" type="INT4" />
            <column name="boolean_value" type="SMALLINT" />
            <column name="text_value" type="VARCHAR(10485760)" />
            <column name="timestamp_value" type="TIMESTAMP WITH TIME ZONE" />
            <column name="binary_value" type="BYTEA" />
            <column name="unit" type="INT8" />
            <column defaultValueComputed="1" name="sequence_number" type="ruleform.sequence_number">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.relationship_attribute" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-302">
        <addForeignKeyConstraint baseColumnNames="attribute" baseTableName="ruleform.relationship_attribute"
            constraintName="relationship_attribute_attribute_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-308">
        <addForeignKeyConstraint baseColumnNames="relationship" baseTableName="ruleform.relationship_attribute"
            constraintName="relationship_attribute_relationship_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-309">
        <addForeignKeyConstraint baseColumnNames="unit" baseTableName="ruleform.relationship_attribute"
            constraintName="relationship_attribute_unit_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.unit" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-310">
        <addForeignKeyConstraint baseColumnNames="updated_by" baseTableName="ruleform.relationship_attribute"
            constraintName="relationship_attribute_updated_by_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION"
            onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-311">
        <addForeignKeyConstraint baseColumnNames="inverse" baseTableName="ruleform.relationship" constraintName="relationship_inverse_fkey"
            deferrable="true" initiallyDeferred="true" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-408">
        <addUniqueConstraint columnNames="relationship, attribute" constraintName="relationship_attribute_relationship_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="ruleform.relationship_attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-514">
        <createIndex indexName="relationship_attribute_updated_by_fkey_idx" tableName="ruleform.relationship_attribute" unique="false">
            <column name="updated_by" />
        </createIndex>
    </changeSet>

    <changeSet id="2" author="hhildebrand">
        <sql> ALTER TABLE ruleform.relationship_attribute OWNER TO core; </sql>
        <rollback />
    </changeSet>
    <changeSet id="4" author="hhildebrand">
        <sql><![CDATA[
            GRANT ALL ON TABLE ruleform.relationship_attribute TO core;
            GRANT SELECT ON TABLE ruleform.relationship_attribute TO core_read_only;
            GRANT SELECT, UPDATE, INSERT, DELETE ON TABLE ruleform.relationship_attribute TO core_read_write;
            ]]>
        </sql>
        <rollback />
    </changeSet>
</databaseChangeLog>
