<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <appender name="C" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.chiralbehaviors.CoRE">
        <level value="TRACE" />
    </logger>

    <root>
        <level value="INFO" />
        <appender-ref ref="C" />
    </root>
</configuration>
