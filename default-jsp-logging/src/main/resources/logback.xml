<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <appender name="C" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <root>
        <level value="WARN" />
        <appender-ref ref="C" />
    </root>
</configuration>
