            <column name="attribute_relationship" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="location_2_attribute" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.location_relationship" />
        </rollback>
    </changeSet>
