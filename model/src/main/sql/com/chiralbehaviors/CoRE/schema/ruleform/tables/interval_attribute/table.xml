<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-18">
        <createTable schemaName="ruleform" tableName="interval_attribute">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="interval_attribute_pkey" />
            </column>
            <column name="interval" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="attribute" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="key" type="VARCHAR(10485760)" />
            <column name="numeric_value" type="numeric" />
            <column name="integer_value" type="INT4" />
            <column name="boolean_value" type="SMALLINT" />
            <column name="text_value" type="VARCHAR(10485760)" />
            <column name="timestamp_value" type="TIMESTAMP WITH TIME ZONE" />
            <column name="binary_value" type="BYTEA" />
            <column name="unit" type="CHAR(22)" />
            <column defaultValueComputed="1" name="sequence_number" type="sequence_number">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" defaultValue="AAAAAAAAAAAAAAAAAAAAAQ" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="interval_attribute" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-145">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="attribute" baseTableName="interval_attribute" constraintName="interval_attribute_attribute_fkey" deferrable="true"
            initiallyDeferred="false" onDelete="CASCADE" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-151">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="interval" baseTableName="interval_attribute" constraintName="interval_attribute_interval_fkey" deferrable="true"
            initiallyDeferred="false" onDelete="CASCADE" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="interval" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-152">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="unit" baseTableName="interval_attribute" constraintName="interval_attribute_unit_fkey" deferrable="true"
            initiallyDeferred="false" onDelete="CASCADE" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="unit" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-153">
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
