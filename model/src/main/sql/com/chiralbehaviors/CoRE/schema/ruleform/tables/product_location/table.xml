            initiallyDeferred="false" onDelete="SET DEFAULT" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-397">
        <addUniqueConstraint schemaName="ruleform" columnNames="agency, product, relationship, location" constraintName="product_location_agency_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="product_location" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-489">
        <createIndex schemaName="ruleform" indexName="product_location_location_fkey_idx" tableName="product_location" unique="false">
