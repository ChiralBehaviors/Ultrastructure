>

    <changeSet author="hhildebrand" id="1390879803225-51">
        <createTable schemaName="ruleform" tableName="relationship_network">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="relationship_network_pkey" />
            </column>
            <column name="parent" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="relationship" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="child" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="inference" type="CHAR(22)" defaultValue="AAAAAAAAAAAAAAAAAAAAAA">
                <constraints nullable="false" />
            </column>
            <column name="premise1" type="CHAR(22)" defaultValue="AAAAAAAAAAAAAAAAAAAAAA">
                <constraints nullable="false" />
            </column>
            <column name="premise2" type="CHAR(22)" defaultValue="AAAAAAAAAAAAAAAAAAAAAA">
                <constraints nullable="false" />
            </column>
            <column name="ordering" type="ordinal_number" />
            <column name="notes" type="TEXT" />
            <column name="updated_by" defaultValue="AAAAAAAAAAAAAAAAAAAAAQ" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="version" type="INT4" />
        </createTable>
        <rollback>
            <dropTable tableName="relationship_network" />
        </rollback>
    </changeSet>
