<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"
>

    <changeSet author="hhildebrand" id="2.0.0">
        <createTable schemaName="ruleform" tableName="unit_network">
            <column name="workspace" type="uuid" />
            <column name="id" type="uuid">
                <constraints primaryKey="true" primaryKeyName="unit_network_pkey" />
            </column>
            <column name="parent" type="uuid">
                <constraints nullable="false" />
            </column>
            <column name="relationship" type="uuid">
                <constraints nullable="false" />
            </column>
            <column name="child" type="uuid">
                <constraints nullable="false" />
            </column>
            <column name="inference" type="uuid" />
            <column name="premise1" type="uuid" />
            <column name="premise2" type="uuid" />
            <column name="ordering" type="ordinal_number" />
            <column name="notes" type="TEXT" />
            <column name="updated_by" type="uuid">
                <constraints nullable="false" />
            </column>
            <column name="version" type="INT4" />
        </createTable>
        <rollback>
            <dropTable tableName="unit_network" />
        </rollback>
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="child" baseTableName="unit_network" constraintName="unit_network_child_fkey" deferrable="true"
            initiallyDeferred="true" onDelete="CASCADE" referencedColumnNames="id" referencedTableName="unit" />
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="parent" baseTableName="unit_network" constraintName="unit_network_parent_fkey" deferrable="true"
            initiallyDeferred="true" onDelete="CASCADE" referencedColumnNames="id" referencedTableName="unit" />
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="relationship" baseTableName="unit_network" constraintName="unit_network_relationship_fkey" deferrable="true"
            initiallyDeferred="true" onDelete="CASCADE" referencedColumnNames="id" referencedTableName="relationship" />
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="updated_by" baseTableName="unit_network" constraintName="unit_network_updated_by_fkey" deferrable="true"
            initiallyDeferred="true" onDelete="SET DEFAULT" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="agency" />
        <addUniqueConstraint schemaName="ruleform" columnNames="parent, relationship, child, inference, premise1, premise2"
            constraintName="unit_network_key" deferrable="false" disabled="false" initiallyDeferred="true" tableName="unit_network" />
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="premise1" baseTableName="unit_network" constraintName="unit_network_premise1_fkey" deferrable="true"
            initiallyDeferred="true" onDelete="CASCADE" referencedColumnNames="id" referencedTableName="unit_network" />
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="premise2" baseTableName="unit_network" constraintName="unit_network_premise2_fkey" deferrable="true"
            initiallyDeferred="true" onDelete="CASCADE" referencedColumnNames="id" referencedTableName="unit_network" />
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="inference" baseTableName="unit_network" constraintName="unit_network_inference_fkey" deferrable="true"
            initiallyDeferred="true" onDelete="CASCADE" referencedColumnNames="id" referencedTableName="network_inference" />
        <addForeignKeyConstraint baseTableSchemaName="ruleform " referencedTableSchemaName="ruleform"
            baseColumnNames="workspace" baseTableName="unit_network" constraintName="unit_network_workspace_fkey" deferrable="true"
            initiallyDeferred="true" onDelete="CASCADE" referencedColumnNames="id" referencedTableName="workspace_authorization" />
    </changeSet>

</databaseChangeLog>
