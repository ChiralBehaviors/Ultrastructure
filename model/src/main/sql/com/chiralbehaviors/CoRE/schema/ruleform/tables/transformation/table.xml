    <changeSet author="hhildebrand" id="1390879803225-58">
        <createTable schemaName="ruleform" tableName="transformation">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="transformation_pkey" />
            </column>
            <column name="service" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="agency" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="product" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="sequence_number" type="sequence_number">
                <constraints nullable="false" />
            </column>
            <column name="assign_to" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="agency_key" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="product_key" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="relationship_key" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="attribute" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="product_attribute_agency" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="transformation" />
        </rollback>
    </changeSet>
