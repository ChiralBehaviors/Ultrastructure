    <changeSet author="hhildebrand" id="1390879803225-23">
        <createTable schemaName="ruleform" remarks="Tracks when status changes occur on any given job" tableName="job_chronology">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="job_chronology_pkey" />
            </column>
            <column name="job" remarks="The Job" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="service" remarks="The service performed by this Job" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="product" remarks="The product produced this Job" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="assign_to" remarks="The agency doing this Job" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="requester" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="deliver_to" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="deliver_from" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="service_attribute" remarks="The attribute service performed by this Job" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="product_attribute" remarks="The product produced this Job" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="assign_to_attribute" remarks="The agency doing this Job" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="requester_attribute" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="deliver_to_attribute" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="deliver_from_attribute" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="status" remarks="The current status of this Job" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="sequence_number" type="INT4">
                <constraints nullable="false" />
            </column>
            <column name="quantity" type="numeric" />
            <column name="quantity_unit" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="notes" remarks="Notes about this job" type="TEXT" />
            <column name="updated_by" defaultValue="AAAAAAAAAAAAAAAAAAAAAQ" remarks="The agency that last updated this rule" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="update_date" defaultValueComputed="('now'::text)::timestamp(6) with time zone" remarks="The date this rule was last changed"
                type="TIMESTAMPTZ"
            >
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="job_chronology" />
        </rollback>
    </changeSet>
