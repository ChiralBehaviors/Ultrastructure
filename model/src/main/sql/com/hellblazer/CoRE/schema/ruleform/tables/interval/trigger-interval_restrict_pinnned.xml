<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="1"
        author="hhildebrand">
        <sql> CREATE TRIGGER interval_restrict_pinned BEFORE UPDATE OR DELETE ON ruleform.interval
            FOR EACH ROW EXECUTE PROCEDURE ruleform.restrict_pinned(); </sql>
        <rollback> DROP TRIGGER interval_restrict_pinned ON ruleform.interval; </rollback>
    </changeSet>
</databaseChangeLog>
