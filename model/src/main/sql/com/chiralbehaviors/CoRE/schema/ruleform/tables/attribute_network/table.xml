>

    <changeSet author="hhildebrand" id="1390879803225-11">
        <createTable tableName="ruleform.attribute_network">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="attribute_network_pkey" />
            </column>
            <column name="parent" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="relationship" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="child" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueNumeric="0" name="inferred" type="SMALLINT">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
            <column name="ordering" type="ruleform.ordinal_number" />
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.attribute_network" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-122">
        <addForeignKeyConstraint baseColumnNames="child" baseTableName="ruleform.attribute_network" constraintName="attribute_network_child_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-123">
        <addForeignKeyConstraint baseColumnNames="parent" baseTableName="ruleform.attribute_network" constraintName="attribute_network_parent_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-124">
        <addForeignKeyConstraint baseColumnNames="relationship" baseTableName="ruleform.attribute_network"
            constraintName="attribute_network_relationship_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE"
            referencedColumnNames="id" referencedTableName="ruleform.relationship" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-125">
        <addForeignKeyConstraint baseColumnNames="updated_by" baseTableName="ruleform.attribute_network"
