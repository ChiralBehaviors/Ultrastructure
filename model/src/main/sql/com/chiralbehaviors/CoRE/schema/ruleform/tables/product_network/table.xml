    <changeSet author="hhildebrand" id="1390879803225-40">
        <createTable schemaName="ruleform" tableName="product_network">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="product_network_pkey" />
            </column>
            <column name="parent" type="CHAR(22)">
