            initiallyDeferred="false" onDelete="SET DEFAULT" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-382">
        <addUniqueConstraint schemaName="ruleform" columnNames="interval, attribute" constraintName="interval_attribute_interval_key"
            deferrable="false" disabled="false" initiallyDeferred="false" tableName="interval_attribute" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-457">
        <createIndex schemaName="ruleform" indexName="interval_attribute_updated_by_fkey_idx" tableName="interval_attribute"
            unique="false"
        >
            <column name="updated_by"   />
        </createIndex>
    </changeSet>
</databaseChangeLog>
