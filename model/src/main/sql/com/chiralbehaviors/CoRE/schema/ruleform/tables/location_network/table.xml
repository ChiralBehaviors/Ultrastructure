<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-27">
        <createTable tableName="ruleform.location_network">
            <column autoIncrement="true" name="id" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="location_network_pkey" />
            </column>
            <column name="parent" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="relationship" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="child" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueNumeric="0" name="inferred" type="SMALLINT">
                <constraints nullable="false" />
            </column>
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
            <column name="ordering" type="ruleform.ordinal_number" />
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.location_network" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-192">
        <addForeignKeyConstraint baseColumnNames="relationship" baseTableName="ruleform.location_network" constraintName="location_network_relationship_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.relationship"/>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-193">
        <addForeignKeyConstraint baseColumnNames="updated_by" baseTableName="ruleform.location_network" constraintName="location_network_updated_by_fkey" deferrable="true" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="CASCADE" referencedColumnNames="id" referencedTableName="ruleform.agency"/>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-389">
        <addUniqueConstraint columnNames="parent, relationship, child" constraintName="location_network_parent_key" deferrable="false" disabled="false" initiallyDeferred="false" tableName="ruleform.location_network"/>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-473">
        <createIndex indexName="location_network_child_fkey_idx" tableName="ruleform.location_network" unique="false">
            <column name="child"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>
