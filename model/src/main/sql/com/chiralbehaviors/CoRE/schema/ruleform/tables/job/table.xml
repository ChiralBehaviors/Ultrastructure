<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd"
>

    <changeSet author="hhildebrand" id="1390879803225-21">
        <createTable remarks="An instantiation of an service; something actually done" tableName="ruleform.job">
            <column autoIncrement="true" name="id" remarks="Unique identifier" type="BIGSERIAL">
                <constraints primaryKey="true" primaryKeyName="job_pkey" />
            </column>
            <column name="service" remarks="The service performed by this Job" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="product" remarks="The product produced this Job" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="assign_to" remarks="The agency doing this Job" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="requester" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="deliver_to" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="deliver_from" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="status" remarks="The current status of this Job" type="INT8">
                <constraints nullable="false" />
            </column>
            <column name="parent" remarks="The parent of this job, in a hierarchical sense" type="INT8" />
            <column defaultValueNumeric="0" name="sequence_number" type="INT4">
                <constraints nullable="false" />
            </column>
            <column name="notes" remarks="Additional notes regarding usage of this particular rule" type="VARCHAR(10485760)" />
            <column name="updated_by" remarks="The agency that last updated this rule" type="INT8">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" remarks="The date this rule was last changed"
                type="TIMESTAMPTZ"
            >
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="ruleform.job" />
        </rollback>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-159">
        <addForeignKeyConstraint baseColumnNames="assign_to" baseTableName="ruleform.job" constraintName="job_assign_to_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-167">
        <addForeignKeyConstraint baseColumnNames="deliver_from" baseTableName="ruleform.job" constraintName="job_deliver_from_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.location" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-168">
        <addForeignKeyConstraint baseColumnNames="deliver_to" baseTableName="ruleform.job" constraintName="job_deliver_to_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.location" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-169">
        <addForeignKeyConstraint baseColumnNames="parent" baseTableName="ruleform.job" constraintName="job_parent_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.job" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-170">
        <addForeignKeyConstraint baseColumnNames="product" baseTableName="ruleform.job" constraintName="job_product_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-171">
        <addForeignKeyConstraint baseColumnNames="requester" baseTableName="ruleform.job" constraintName="job_requester_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-172">
        <addForeignKeyConstraint baseColumnNames="service" baseTableName="ruleform.job" constraintName="job_service_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.product" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-173">
        <addForeignKeyConstraint baseColumnNames="status" baseTableName="ruleform.job" constraintName="job_status_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.status_code" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-174">
        <addForeignKeyConstraint baseColumnNames="updated_by" baseTableName="ruleform.job" constraintName="job_updated_by_fkey"
            deferrable="true" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="CASCADE" referencedColumnNames="id"
            referencedTableName="ruleform.agency" />
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-463">
        <createIndex indexName="job_assign_to_fkey_idx" tableName="ruleform.job" unique="false">
            <column name="assign_to" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-466">
        <createIndex indexName="job_parent_fkey_idx" tableName="ruleform.job" unique="false">
            <column name="parent" />
        </createIndex>
    </changeSet>
</databaseChangeLog>
