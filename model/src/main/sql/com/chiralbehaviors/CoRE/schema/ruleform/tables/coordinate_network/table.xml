    <changeSet author="hhildebrand" id="1390879803225-16">
        <createTable schemaName="ruleform" tableName="coordinate_network">
            <column name="id" type="CHAR(22)">
                <constraints primaryKey="true" primaryKeyName="coordinate_network_pkey" />
            </column>
            <column name="parent" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="relationship" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="child" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column name="inferred" type="SMALLINT" defaultValueNumeric="0">
                <constraints nullable="false" />
            </column> 
            <column name="premise1" type="CHAR(22)" defaultValue="AAAAAAAAAAAAAAAAAAAAAA">
                <constraints nullable="false" />
            </column>
            <column name="premise2" type="CHAR(22)" defaultValue="AAAAAAAAAAAAAAAAAAAAAA">
                <constraints nullable="false" />
            </column> 
            <column name="ordering" type="ordinal_number" />
            <column name="notes" type="VARCHAR(10485760)" />
            <column name="updated_by" type="CHAR(22)">
                <constraints nullable="false" />
            </column>
            <column defaultValueComputed="('now'::text)::timestamp(6) with time zone" name="update_date" type="TIMESTAMPTZ">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="coordinate_network" />
        </rollback>
    </changeSet>
