    <changeSet author="hhildebrand" id="1390879803225-493">
        <createIndex schemaName="ruleform" indexName="product_location_product_fkey_idx" tableName="product_location" unique="false">
            <column name="product" />
        </createIndex>
    </changeSet>
    <changeSet author="hhildebrand" id="1390879803225-494">
